<%= form_for @users do |u| %>
<%= u.label :username %>
<%= u.text_field :username %><br/>

<%= u.label :email %>
<%= u.text_field :email %><br/>

<%= u.label :f_name %>
<%= u.text_field :f_name %><br/>

<%= u.label :l_name %>
<%= u.text_field :l_name %><br/>

<%= u.label :full_name %>
<%= u.text_field :full_name %><br/>

<%= u.label :contact_no %>
<%= u.text_field :contact_no %><br/>

<%= u.label :user_type %>
<%= u.text_field :user_type %><br/>
</br>
<%= u.submit %>
<% end %>


